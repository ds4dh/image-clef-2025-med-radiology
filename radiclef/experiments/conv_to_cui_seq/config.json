{
  "session": {
    "device_name": "cuda:0",
    "num_epochs": 500,
    "mini_batch_size": 86,
    "learning_rate": 0.003,
    "weight_decay": 1e-5,
    "dataloader_num_workers": 8,
    "tag_postfix": null
  },
  "data": {
    "image_size": [
      1024,
      1024
    ],
    "image_positional_embedding": true,
    "image_mode": "RGB",
    "image_augment_transforms": {
      "do_transforms": true,
      "random_linear_illumination": {
        "p": 0.3,
        "gain": 0.1
      },
      "random_adjust_sharpness": {
        "p": 0.1,
        "sharpness_factor": 2
      },
      "jitter": {
        "p": 0.1,
        "brightness": 0.1,
        "saturation": 0.0,
        "contrast": 0.1
      },
      "random_resized_crop": {
        "p": 0.1,
        "ratio": [
          0.8,
          1.2
        ],
        "scale": [
          0.25,
          1.5
        ]
      },
      "random_rotation": {
        "p": 0.4,
        "degrees": [
          -45,
          45
        ]
      }
    }
  },
  "metrics": {
    "F1Metric": [
      "f1_score"
    ]
  },
  "network": {
    "architecture": "ConvEmbeddingToSec",
    "convolutional_embedding": {
      "sampling_ratio_list": [
        2,
        4,
        4,
        4
      ],
      "channels_list": [
        5,
        8,
        8,
        8,
        16
      ],
      "num_out_channels": 1,
      "dropout": 0.1
    },
    "sequence_generator": {
      "input_dim": 64,
      "hidden_dim": 32,
      "max_len": 32,
      "num_layers": 2,
      "num_heads": 1,
      "dim_feedforward": 32,
      "dropout": 0.1
    }
  }
}
